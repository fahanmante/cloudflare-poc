{
    "headers": [
        {
            "source": "/(.*)\\.(lottie|svg|jpg|jpeg|png|gif|ico|webp|avif|css|js|woff2|ttf)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
                },
                {
                    "key": "Vercel-CDN-Cache-Control",
                    "value": "no-store"
                }
            ]
        }
    ]
}